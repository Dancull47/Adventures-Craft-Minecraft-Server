on tab complete of "/Tag":
	set tab completions for position 2 to "Default", "Explorer", "Adventurer", "Conquerer"
	set tab completions for position 1 to "Default", "Explorer", "Adventurer", "Conquerer", "FarmingP1", "FarmingP2", "FarmingP3", "FarmingP4", "FarmingP5", "ForagingP1", "ForagingP2", "ForagingP3", "ForagingP4", "ForagingP5", "MiningP1", "MiningP2", "MiningP3", "MiningP4", "MiningP5", "SlayerP1", "SlayerP2", "SlayerP3", "SlayerP4", "SlayerP5", "EnchantingP1", "EnchantingP2", "EnchantingP3", "EnchantingP4", "EnchantingP5", "SpellforgingP1", "SpellforgingP2", "SpellforgingP3", "SpellforgingP4", "SpellforgingP5"

command /Tag [<text>] [<text>]:
	description: Battle Tags
	usage: Used for toggling Battle Tags!
	executable by: players
	trigger:
		if arg 1 is not set:    
			execute console command "/dm open BattleTags %player%"
		if arg 1 is set:    
			if arg 2 is not "Default" or "Explorer" or "Adventurer" or "Conquerer":    
				message "&cYou must choose either &7Default&c, &aExplorer&c, &9Adventurer&c, or Conquerer!"

		if arg 1 is "Default":
			if arg 2 is "Default":
				tag_DefaultD(player)
			else if arg 2 is "Explorer":
				tag_DefaultE(player)
			else if arg 2 is "Adventurer":
				tag_DefaultA(player)
			else if arg 2 is "Conquerer":
				tag_DefaultC(player)
		else if arg 1 is "Explorer":
