						set the result to "4"
						if player has permission "SLAYER.PRESTIGE5":
							set the result to "5"
		else:
			set the result to "0"

	if the identifier is "spellforging_prestige":
		if player has permission "SPELLFORGING.PRESTIGE1":
			set the result to "1"
			if player has permission "SPELLFORGING.PRESTIGE2":
				set the result to "2"
				if player has permission "SPELLFORGING.PRESTIGE3":
					set the result to "3"
					if player has permission "SPELLFORGING.PRESTIGE4":
						set the result to "4"
						if player has permission "SPELLFORGING.PRESTIGE5":
							set the result to "5"
		else:
			set the result to "0"

	if the identifier is "enchanting_prestige":
		if player has permission "ENCHANTING.PRESTIGE1":
			set the result to "1"
			if player has permission "ENCHANTING.PRESTIGE2":
				set the result to "2"
				if player has permission "ENCHANTING.PRESTIGE3":
					set the result to "3"
					if player has permission "ENCHANTING.PRESTIGE4":
						set the result to "4"
						if player has permission "ENCHANTING.PRESTIGE5":
							set the result to "5"
		else:
			set the result to "0"

	if the identifier is "Quests1":
		set {_XP} to placeholder "mmocore_profession_experience_Enchanting" from player
		set {_XP2} to (1000 - {_XP} parsed as number)
		set the result to "%{_XP2}% + 1 %{_XP}%"
