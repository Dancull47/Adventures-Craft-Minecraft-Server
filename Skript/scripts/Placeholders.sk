on placeholderapi placeholder request for the prefix "custom":
	if the identifier is "farming_prestige":
		if player has permission "FARMING.PRESTIGE1":
			set the result to "1"
			if player has permission "FARMING.PRESTIGE2":
				set the result to "2"
				if player has permission "FARMING.PRESTIGE3":
					set the result to "3"
					if player has permission "FARMING.PRESTIGE4":
						set the result to "4"
						if player has permission "FARMING.PRESTIGE5":
							set the result to "5"
		else:
			set the result to "0"

	if the identifier is "foraging_prestige":
